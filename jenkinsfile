pipeline {
	agent ant
	
	stages {
		stage ('compile stage') {
			steps {
				withMaven(maven : 'maven3.6') {
					sh 'mvn clean compile'
				}
			}
		}
		
		stage ('Testing stage') {
			steps {
				withMaven(maven : 'maven3.6') {
					sh 'mvn test'
				}
			}
		}
		
		stage ('Deployment stage') {
			steps {
				withMaven(maven : 'maven3.6') {
					sh 'mvn deployment test'
				}
			}
		}
		
				